generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Channel {
  id         String       @id @unique
  name       String
  link       String
  externalId String
  LiveStream LiveStream[]
}

model LiveStream {
  id          String  @id @unique
  title       String
  description String
  externalId  String
  startedAt   String
  endedAt     String?
  channelId   String
  channel     Channel @relation(fields: [channelId], references: [id], onDelete: Cascade)

  @@index([channelId], map: "LiveStream_channelId_fkey")
}
